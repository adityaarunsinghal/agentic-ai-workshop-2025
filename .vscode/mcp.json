{
	"servers": {
		"weather-mcp-server": {
			"type": "stdio",
			"command": "uv",
			"args": [
				"run",
				"--directory",
				"{your_full_path_to_this_repo}",
				"python",
				"src/server/weather_server.py"
			]
		},
		"news-agent-server": {
			"type": "http",
			"url": "http://localhost:8080/mcp"
		},
		"preference-agent-server": {
			"type": "http",
			"url": "http://localhost:8082/mcp"
		}
	},
	"inputs": []
}
